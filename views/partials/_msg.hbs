{{!-- Alert req flash success --}}
{{#if success_msg}}
   <div class="row justify-content-center">
        <div class="alert alert-success col-8 mt-3" role="alert">{{success_msg}}</div>
   </div>
{{/if}}
{{!-- Alert req flash error --}}
{{#if error_msg}}
    <div class="row justify-content-center">
        <div class="alert alert-danger col-8 mt-3" role="alert">{{error_msg}}</div>
    </div>
{{/if}}